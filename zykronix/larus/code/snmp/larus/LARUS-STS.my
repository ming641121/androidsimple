--
--	stsEmmStatusTable RTRV-Emm for Emm status. ED-Emm for provisioning Emm thresholds.
--

stsEmmStatusTable  OBJECT-TYPE
	SYNTAX		SEQUENCE OF StsEmmStatusEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION   "the current state of Emm conditions.
        "
	::= { stsEmmStatus 1 }

stsEmmStatusEntry OBJECT-TYPE
	SYNTAX      StsEmmStatusEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION   "Entry of the current state of Emm conditions.
        "
	INDEX	{ stsEmmShelf, stsEmmSensorType
}
	::= { stsEmmStatusTable 1 }

StsEmmStatusEntry ::= SEQUENCE {
	stsEmmShelf
		INTEGER,
        stsEmmSensorType
		INTEGER,
        stsEmmAlarmStatus
		INTEGER,
	stsEmmSensorReading
		INTEGER,
        stsEmmMajorAlmHiThresh
		INTEGER,
        stsEmmMajorAlmLoThresh
		INTEGER,
        stsEmmMinorAlmHiThresh
		INTEGER,
        stsEmmMinorAlmLoThresh
		INTEGER
}


stsEmmShelf  OBJECT-TYPE
    SYNTAX      INTEGER {
					main(1),
					expansion-1(2),
					expansion-2(3),
					expansion-3(4),
					expansion-4(5)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Output shelf number. Shelf 0 is the main chassis.
        "
    ::= { stsEmmStatusEntry 1 }

stsEmmSensorType  OBJECT-TYPE
    SYNTAX      INTEGER {
					temp-ambient(1),
					volt5(2),
					volt12(3),
					volt48-A(4),
					volt48-B(5)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Sensor type.
        "
    ::= { stsEmmStatusEntry 2 }

stsEmmAlarmStatus  OBJECT-TYPE
    SYNTAX      INTEGER {
					cr(1),
					mj(2),
					mn(3),
					na(4),
					nr(5),
					cl(6)

				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Current alarm level.

cr - Critical alarm
mj - Major alarm
mn - Minor alarm
na - Not alarmed; report as event
nr - Not reported
cl - No alarm or alarm cleared; also used in trap
        "
    ::= { stsEmmStatusEntry 3 }


stsEmmSensorReading  OBJECT-TYPE
    SYNTAX      INTEGER (-50..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "  The current Emm sensor reading.
        "
    ::= { stsEmmStatusEntry 4 }

stsEmmMajorAlmHiThresh  OBJECT-TYPE
    SYNTAX      INTEGER (-50..100)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "  The setting value of the majoralarm high threshold of this sensor. 
                   Not applicable to voltage-mvolt.
        "
    ::= { stsEmmStatusEntry 5 }

stsEmmMajorAlmLoThresh  OBJECT-TYPE
    SYNTAX      INTEGER (-50..100)
    MAX-ACCESS  read-write 
    STATUS      current
    DESCRIPTION "  The setting value of the major alarm low threshold of this sensor.
                  Not applicable to voltage-mvolt.
        "
    ::= { stsEmmStatusEntry 6 }

stsEmmMinorAlmHiThresh  OBJECT-TYPE
    SYNTAX      INTEGER (-50..100)
    MAX-ACCESS  read-write 
    STATUS      current
    DESCRIPTION "  The setting value of the minor alarm high threshold of this sensor.
                   Not applicable to voltage-mvolt.
        "
    ::= { stsEmmStatusEntry 7 }


stsEmmMinorAlmLoThresh  OBJECT-TYPE
    SYNTAX      INTEGER (-50..100)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "  The setting value of the minor alarm low threshold of this sensor.
                  Not applicable to voltage-mvolt.
        "
    ::= { stsEmmStatusEntry 8 }


