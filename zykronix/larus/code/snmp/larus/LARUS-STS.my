--
--	stsAlarmThresholdTable SET-TH-LINK and RTRV-TH-LINK 
--

stsAlarmThresholdTable  OBJECT-TYPE
	SYNTAX		SEQUENCE OF StsAlarmThresholdEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION   "Alarm threshold table.
        "
	::= { stsAlarmThreshold 1 }

stsAlarmThresholdEntry OBJECT-TYPE
	SYNTAX      StsAlarmThresholdEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION   "Alarm threshold entry.
        "
	INDEX	{ stsAlarmThresholdCurrentMon, stsAlarmThresholdType }
	::= { stsAlarmThresholdTable 1 }

StsAlarmThresholdEntry ::= SEQUENCE {
	stsAlarmThresholdCurrentMon
		INTEGER,
	stsAlarmThresholdType
		INTEGER,
    stsAlarmThresholdValue
		INTEGER,
    stsAlarmThresholdTimeInterval
		INTEGER
 
}


stsAlarmThresholdCurrentMon  OBJECT-TYPE
    SYNTAX      INTEGER {
					link1(1),
					link2(2),
					link3(3),
					link4(4),
					link5(5)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Current monitor number.
        "
    ::= { stsAlarmThresholdEntry 1 }


stsAlarmThresholdType  OBJECT-TYPE
    SYNTAX      INTEGER {
					cvl(1),
					cvp(2),
					cs(3),
					jitter(4),
					tie(5),
					mtie(6)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Monitor alarm threshold setting type.
CVL - Coding violation count - Line (BPV)
CVP - Coding violation count - Path (CRC)
CS - Controlled slip count
JITTER - Peak to peak JITTER in unit interval
TIE - Time interval error in nanoseconds
MTIE - Maximum time interval error in nanoseconds
        "
    ::= { stsAlarmThresholdEntry 2 }

stsAlarmThresholdValue  OBJECT-TYPE
    SYNTAX      INTEGER   (0..10000)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Threshold level to set. When this value is reached, a trap will be sent out. Below are the default values:

CVL - 16
CVP - 16
CS - 1
JITTER - 5 UI (T1)
1 - UI (E1)
TIE (Stratum 1) - 0.503 UI (T1), 0.668 UI (E1) - use 503 and 668 as the integer values. 
MTIE (Stratum 1) - 0.503 UI (T1), 0.668 UI (E1) - use 503 and 668 as the integer values.
        "
    ::= { stsAlarmThresholdEntry 3 }

stsAlarmThresholdTimeInterval  OBJECT-TYPE
    SYNTAX      INTEGER {
					one-Sec(1),
					ten-Sec(2),
					hundred-Sec(3),
					thousand-Sec(4),
					tenThousand-Sec(5),
					hundredThousand-Sec(6),
					one-hour(7)
				}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Time interval setting for the corresponding alarm threshold value. Below are the default values.

CVL - ten-Sec
CVP - ten-Sec
CS - one-Hour
JITTER - 1-Sec
TIE (Stratum 1) - one-Hour
MTIE (Stratum 1) - one-Hour
wander - hundred-Sec
        "
    ::= { stsAlarmThresholdEntry 4 }



--
--	stsLinkAlarmTable SET-ATTR and RTRV-ATTR for LINK type
--

stsLinkAlarmTable  OBJECT-TYPE
	SYNTAX		SEQUENCE OF StsLinkAlarmEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION   "Link alarm setting table
        "
	::= { stsLinkAlarmSetting 1 }

stsLinkAlarmEntry OBJECT-TYPE
	SYNTAX      StsLinkAlarmEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION   "Link alarm setting entry
        "
	INDEX	{ stsLinkAlarmSettingLinkType, stsLinkAlarmSettingType }
	::= { stsLinkAlarmTable 1 }

StsLinkAlarmEntry ::= SEQUENCE {
	stsLinkAlarmSettingLinkType
		INTEGER,
	stsLinkAlarmSettingType
		INTEGER,
    stsLinkAlarmSettingSevirity
		INTEGER,
  stsLinkAlarmSevirity
		INTEGER

}


stsLinkAlarmSettingLinkType  OBJECT-TYPE
    SYNTAX      INTEGER {
					link1(1),
					link2(2),
					link3(3),
					link4(4),
					link5(5),
					f5F10(6),
					plnk(7),
					slnk(8)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Link type for the alarm setting
        "
    ::= { stsLinkAlarmEntry 1 }


stsLinkAlarmSettingType  OBJECT-TYPE
    SYNTAX      INTEGER {
					act(1),
					los(2),
					aisp(3),
					lof(4),
					cvl(5),
					cvp(6),
					cs(7),
					jitter(8),
					tie(9),
					mtie(10)


				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Alarm setting type. 
ACT (NR, NSA) Link in active state
AISP (MN, SA) Alarm indication signal detected - Path. N/A to F2 and F5F10 link types.
LOF (MN, SA) Loss of frame. N/A to F2 and F5F10 link types.
LOS (MN, SA) Loss of signal
T-CS (NA, NSA) Threshold violation for MONTYPE CS. N/A to F2, F5F10, PLNK and SLNK link types.
T-TIE (NA, NSA) Threshold violation for MONTYPE TIE. N/A to F5F10, PLNK and SLNK link types.
T-MTIE (NA, NSA) Threshold violation for MONTYPE MTIE. N/A to F5F10, PLNK and SLNK link types.

        "
    ::= { stsLinkAlarmEntry 2 }

stsLinkAlarmSettingSevirity  OBJECT-TYPE
    SYNTAX      INTEGER {
					cr(1),
					mj(2),
					mn(3),
					na(4),
					nr(5)

				}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Threshold level to set. When this value is reached, a trap will be sent out.

cr - Critical alarm
mj - Major alarm
mn - Minor alarm
na - Not alarmed; report as event
nr - Not reported
        "
    ::= { stsLinkAlarmEntry 3 }

