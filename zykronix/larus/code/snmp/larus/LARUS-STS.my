--- TOP Destination table

 stsTopDestTable OBJECT-TYPE 
	SYNTAX		SEQUENCE OF StsTopDestEntry
	MAX-ACCESS	not-accessible
	STATUS		current
 DESCRIPTION "The table has the information on the manager receiving traps 
              " 
 ::= {  stsTopDest 1 } 
 
 stsTopDestEntry OBJECT-TYPE 
 SYNTAX       StsTopDestEntry
 MAX-ACCESS       not-accessible
 STATUS       current
 DESCRIPTION "An entry for the manager 
              " 
 INDEX { stsTopDestNumber } 
 ::= { stsTopDestTable 1 } 
 
 StsTopDestEntry ::= 
 SEQUENCE { 
            stsTopDestNumber                  INTEGER,
            stsTopDestIp                      IpAddress,
            stsTopDestUdpPort                 INTEGER,
	      stsTopDestVlanId			       INTEGER
          }
          
 stsTopDestNumber OBJECT-TYPE 
 SYNTAX       INTEGER (1..28)
 MAX-ACCESS       read-only
 STATUS       current
 DESCRIPTION "A device ID of this TOP entry in the table. 
              " 
 ::= { stsTopDestEntry 1 }    
         
 stsTopDestIp OBJECT-TYPE 
 SYNTAX       IpAddress
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "IP address of the destination. 
              " 
 ::= { stsTopDestEntry 2 }  
 
 stsTopDestUdpPort OBJECT-TYPE 
 SYNTAX       INTEGER (0..65535)
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "UDP port number of the destination.
              " 
 ::= { stsTopDestEntry 3 } 
 

 stsTopDestVlanId OBJECT-TYPE 
 SYNTAX       INTEGER (1..32)
 MAX-ACCESS   read-write
 STATUS       current
 DESCRIPTION "VLAN ID for this TOP" 
 
 ::= { stsTopDestEntry 4 } 
 


--- trap receiver table

 stsTrapRegTable OBJECT-TYPE 
 SYNTAX       SEQUENCE OF  StsTrapRegEntry 
 MAX-ACCESS       not-accessible
 STATUS       current
 DESCRIPTION "The table has the information on the manager receiving traps 
              " 
 ::= {  stsTrapReg 1 } 
 
 stsTrapRegEntry OBJECT-TYPE 
 SYNTAX       StsTrapRegEntry
 MAX-ACCESS       not-accessible
 STATUS       current
 DESCRIPTION "An entry for the manager 
              " 
 INDEX { stsTrapRegMgrNumber } 
 ::= { stsTrapRegTable 1 } 
 
 StsTrapRegEntry ::= 
 SEQUENCE { 
            stsTrapRegMgrNumber                  INTEGER,
            stsTrapRegMgrIp                      IpAddress,
            stsTrapRegMgrUdpPort                 INTEGER,
            stsTrapRegCommunityStr               DisplayString,
	      stsTrapRegState			    INTEGER
          }
          
 stsTrapRegMgrNumber OBJECT-TYPE 
 SYNTAX       INTEGER (1..10)
 MAX-ACCESS       read-only
 STATUS       current
 DESCRIPTION "A unique number used as an index to the table. 
              " 
 ::= { stsTrapRegEntry 1 }    
         
 stsTrapRegMgrIp OBJECT-TYPE 
 SYNTAX       IpAddress
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "IP address of the trap receiver manager. 
              " 
 ::= { stsTrapRegEntry 2 }  
 
 stsTrapRegMgrUdpPort OBJECT-TYPE 
 SYNTAX       INTEGER (0..65535)
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "UDP port number on which the manager receives traps from an agent. 
              " 
 
 ::= { stsTrapRegEntry 3 } 
 

 stsTrapRegCommunityStr OBJECT-TYPE 
 SYNTAX       DisplayString (SIZE  (0..24))
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "This object is the community string(up to 20 characters)to be 
              carried by the traps sent to the registered manager. Each mamanger 
              can have different community string.
              " 
 ::= { stsTrapRegEntry 4 } 

 stsTrapRegState OBJECT-TYPE 
 SYNTAX       INTEGER {
				enabled(1),
				disabled(2)
			  }
 MAX-ACCESS   read-write
 STATUS       current
