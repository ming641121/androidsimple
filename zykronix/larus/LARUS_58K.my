--
--	sts58kAntTable  SW-ANT, SET-ANT
--

sts58kAntTable  OBJECT-TYPE
	SYNTAX		SEQUENCE OF Sts58kAntEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION   "Antenna table
        "
	::= { sts58kAnt 1 }

sts58kAntEntry OBJECT-TYPE
	SYNTAX      Sts58kAntEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION   "Antenna entry.
        "
	INDEX	{ sts58kAntIndex }
	::= { sts58kAntTable 1 }

Sts58kAntEntry ::= SEQUENCE {
	sts58kAntIndex 
		INTEGER,
	sts58kAntStatus 
		INTEGER,
	sts58kAntSwitchState 
		INTEGER
}

sts58kAntIndex  OBJECT-TYPE
    SYNTAX      INTEGER (1..2)
    MAX-ACCESS  read-only
    STATUS      current 
    DESCRIPTION "Ant module index
        "
    ::= { sts58kAntEntry 1 }

sts58kAntStatus  OBJECT-TYPE
    SYNTAX      INTEGER {
					active(1),
					standby(2),
					failure(3),
					absent(4)

				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Ant current status
        "
    ::= { sts58kAntEntry 2 }

sts58kAntSwitchState  OBJECT-TYPE
    SYNTAX      INTEGER {
                              noAction(0),
					active(1),
					standby(2)

				}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Provisioning this Ant to be active or standby. The other Ant (if existed) will change its active/standby state accordingly.
        "
    ::= { sts58kAntEntry 3 }


--- TOP Destination table

 sts58kTopDestTable OBJECT-TYPE 
	SYNTAX		SEQUENCE OF Sts58kTopDestEntry
	MAX-ACCESS	not-accessible
	STATUS		current
 DESCRIPTION "The table has the information on the manager receiving traps 
              " 
 ::= {  sts58kTopDest 1 } 
 
 sts58kTopDestEntry OBJECT-TYPE 
 SYNTAX       Sts58kTopDestEntry
 MAX-ACCESS       not-accessible
 STATUS       current
 DESCRIPTION "An entry for the manager 
              " 
 INDEX { sts58kTopDestNumber } 
 ::= { sts58kTopDestTable 1 } 
 
 Sts58kTopDestEntry ::= 
 SEQUENCE { 
            sts58kTopDestNumber                  INTEGER,
            sts58kTopDestIp                      IpAddress,
            sts58kTopDestUdpPort                 INTEGER,
	      sts58kTopDestVlanId			       INTEGER
          }
          
 sts58kTopDestNumber OBJECT-TYPE 
 SYNTAX       INTEGER (1..28)
 MAX-ACCESS       read-only
 STATUS       current
 DESCRIPTION "A device ID of this TOP entry in the table. 
              " 
 ::= { sts58kTopDestEntry 1 }    
         
 sts58kTopDestIp OBJECT-TYPE 
 SYNTAX       IpAddress
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "IP address of the destination. 
              " 
 ::= { sts58kTopDestEntry 2 }  
 
 sts58kTopDestUdpPort OBJECT-TYPE 
 SYNTAX       INTEGER (0..65535)
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "UDP port number of the destination.
              " 
 ::= { sts58kTopDestEntry 3 } 
 

 sts58kTopDestVlanId OBJECT-TYPE 
 SYNTAX       INTEGER (1..32)
 MAX-ACCESS   read-write
 STATUS       current
 DESCRIPTION "VLAN ID for this TOP" 
 
 ::= { sts58kTopDestEntry 4 } 
 


--- trap receiver table

 sts58kTrapRegTable OBJECT-TYPE 
 SYNTAX       SEQUENCE OF  Sts58kTrapRegEntry 
 MAX-ACCESS       not-accessible
 STATUS       current
 DESCRIPTION "The table has the information on the manager receiving traps 
              " 
 ::= {  sts58kTrapReg 1 } 
 
 sts58kTrapRegEntry OBJECT-TYPE 
 SYNTAX       Sts58kTrapRegEntry
 MAX-ACCESS       not-accessible
 STATUS       current
 DESCRIPTION "An entry for the manager 
              " 
 INDEX { sts58kTrapRegMgrNumber } 
 ::= { sts58kTrapRegTable 1 } 
 
 Sts58kTrapRegEntry ::= 
 SEQUENCE { 
            sts58kTrapRegMgrNumber                  INTEGER,
            sts58kTrapRegMgrIp                      IpAddress,
            sts58kTrapRegMgrUdpPort                 INTEGER,
            sts58kTrapRegCommunityStr               DisplayString,
	      sts58kTrapRegState			    INTEGER
          }
          
 sts58kTrapRegMgrNumber OBJECT-TYPE 
 SYNTAX       INTEGER (1..10)
 MAX-ACCESS       read-only
 STATUS       current
 DESCRIPTION "A unique number used as an index to the table. 
              " 
 ::= { sts58kTrapRegEntry 1 }    
         
 sts58kTrapRegMgrIp OBJECT-TYPE 
 SYNTAX       IpAddress
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "IP address of the trap receiver manager. 
              " 
 ::= { sts58kTrapRegEntry 2 }  
 
 sts58kTrapRegMgrUdpPort OBJECT-TYPE 
 SYNTAX       INTEGER (0..65535)
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "UDP port number on which the manager receives traps from an agent. 
              " 
 
 ::= { sts58kTrapRegEntry 3 } 
 

 sts58kTrapRegCommunityStr OBJECT-TYPE 
 SYNTAX       DisplayString (SIZE  (0..24))
 MAX-ACCESS       read-write
 STATUS       current
 DESCRIPTION "This object is the community string(up to 20 characters)to be 
              carried by the traps sent to the registered manager. Each mamanger 
              can have different community string.
              " 
 ::= { sts58kTrapRegEntry 4 } 

 sts58kTrapRegState OBJECT-TYPE 
 SYNTAX       INTEGER {
				enabled(1),
				disabled(2)
			  }
 MAX-ACCESS   read-write
 STATUS       current
 DESCRIPTION "The state of the Trap IP. Trap will not sent to this trap manager if disabled" 
 ::= { sts58kTrapRegEntry 5 } 
 


END



