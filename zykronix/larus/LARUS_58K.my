sts58kLinkType  OBJECT-TYPE
    SYNTAX      INTEGER {
					e1(1),
					t1(2),
					cc(3),
					f2(4)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Identify the input link type.
        "
    ::= { sts58kLinkEntry 2 }

sts58kLinkEnable  OBJECT-TYPE
    SYNTAX      INTEGER {
					yes(1),
					no(2)

				}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Cab be configured on link 3, 4 and 5.
        "
