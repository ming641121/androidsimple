	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION   "Link alarm setting entry
        "
	INDEX	{ sts58kLinkAlarmSettingLinkType, sts58kLinkAlarmSettingType }
	::= { sts58kLinkAlarmSettingTable 1 }

Sts58kLinkAlarmSettingEntry ::= SEQUENCE {
	sts58kLinkAlarmSettingLinkType
		INTEGER,
	sts58kLinkAlarmSettingType
		INTEGER,
    sts58kLinkAlarmSettingSevirity
		INTEGER
}


sts58kLinkAlarmSettingLinkType  OBJECT-TYPE
    SYNTAX      INTEGER {
					mon1(1),
					mon2(2),
					mon3(3),
					mon4(4),
					mon5(5),
					f5F10(6),
					plnk(7),
					slnk(8)
				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Link type for the alarm setting
        "
    ::= { sts58kLinkAlarmSettingEntry 1 }


sts58kLinkAlarmSettingType  OBJECT-TYPE
    SYNTAX      INTEGER {
					act(1),
					los(2),
					t-tie(3),
					t-mtie(4),
					aisp(5),
					lof(6),
					t-cs(7)

				}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Alarm setting type. 
ACT (NR, NSA) Link in active state
AISP (MN, SA) Alarm indication signal detected - Path. N/A to F2 and F5F10 link types.
LOF (MN, SA) Loss of frame. N/A to F2 and F5F10 link types.
LOS (MN, SA) Loss of signal
T-CS (NA, NSA) Threshold violation for MONTYPE CS. N/A to F2, F5F10, PLNK and SLNK link types.
T-TIE (NA, NSA) Threshold violation for MONTYPE TIE. N/A to F5F10, PLNK and SLNK link types.
T-MTIE (NA, NSA) Threshold violation for MONTYPE MTIE. N/A to F5F10, PLNK and SLNK link types.

        "
    ::= { sts58kLinkAlarmSettingEntry 2 }

sts58kLinkAlarmSettingSevirity  OBJECT-TYPE
    SYNTAX      INTEGER {
					cr(1),
					mj(2),
					mn(3),
					na(4),
					nr(5)

				}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Threshold level to set. When this value is reached, a trap will be sent out.

cr - Critical alarm
mj - Major alarm
mn - Minor alarm
na - Not alarmed; report as event
nr - Not reported
        "
    ::= { sts58kLinkAlarmSettingEntry 3 }




